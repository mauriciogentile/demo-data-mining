<Window x:Class="FileImporter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="600" Width="800">
    <Grid>
        <DataGrid HorizontalAlignment="Left" ItemsSource="{Binding}"  Margin="10,80,0,0" VerticalAlignment="Top" Height="480" Width="772"/>
        <Button Content="Arhivo..." HorizontalAlignment="Left" Margin="707,10,0,0" VerticalAlignment="Top" Width="75" Click="PickFile"/>
        <TextBox Name="TextBox1" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="692"/>
        <Button Content="Procesar" HorizontalAlignment="Left" Margin="707,53,0,0" VerticalAlignment="Top" Width="75" Click="ProcessFile"/>
    </Grid>
</Window>
